<% IF prev or next %>
<div class="prev-next">
<h2>
<% IF prev %>
  <span class="label label-lg label-success prev">
    <a href="<% prev.url %>" title="<% prev.title %>"><span class="glyphicon glyphicon-chevron-left"></span> Prev</a>
  </span>
<% END %>
<% IF next %>
  <span class="label label-lg label-success next">
    <a href="<% next.url %>" title="<% next.title %>">Next <span class="glyphicon glyphicon-chevron-right"></span> </a>
  </span>
<% END %></h2>
</div>
<% END %>
